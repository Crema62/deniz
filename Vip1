--------MAIN MENU AS MULTICHOICE------------

function Main()
local t = gg.multiChoice({
'Radar',
'Hitbox',
'No Flash',
'No Spread',
'Fire Rate',
'No Recoil',
'No Aimpunch',
'FOV',
'Lag Hack',
'EXIT'},
nil,[[ ðŸ‘‘Team CremaðŸ‘‘
Critical Ops 1.21 (64bit) Script ]])
if t == nil then gg.sleep(1)
else
if t[1] then hacka() end
if t[2] then hitbox() end
if t[3] then flash() end
if t[4] then spread() end
if t[5] then hackbb() end
if t[6] then hackc() end
if t[7] then aim() end
if t[8] then hack2() end
if t[9] then lag() end
if t[10] then Exit() end
end
XGCK=-1
end




function hacka()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber("hC0 02 00 36 74 00", gg.TYPE_BYTE, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(12)
gg.editAll("hC0 02 18 36 74 00", gg.TYPE_BYTE)
gg.clearResults()


gg.setRanges(gg.REGION_C_HEAP | gg.REGION_C_ALLOC | gg.REGION_C_DATA | gg.REGION_C_BSS | gg.REGION_PPSSPP | gg.REGION_ANONYMOUS)
gg.searchNumber("0", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()

local t = gg.getResults(100000, nil, nil, nil, nil, nil, nil, nil, nil)
gg.addListItems(t)
t = nil
gg.clearList()
gg.clearResults()
gg.toast('Radar ON')
end

