gg.getResults(1)
gg.editAll("1", gg.TYPE_FLOAT)
gg.clearResults()





gg.setRanges(gg.REGION_C_HEAP | gg.REGION_C_ALLOC | gg.REGION_C_DATA | gg.REGION_C_BSS | gg.REGION_PPSSPP | gg.REGION_ANONYMOUS)
