--------MAIN MENU AS MULTICHOICE------------

function Main()
local t = gg.multiChoice({
'▫Radar',
'▫Hitbox',
'▫No Flash',
'▫No Smoke',
'▫No Spread',
'▫Fire Rate',
'▫No Recoil',
'▫No Aimpunch',
'▫FOV',
'❌EXIT❌'},
nil,[[ 👑Team Crema Vip Script👑
✅Game: Critical Ops
✅Version: 1.22.0.f1288
✅Bit: 32 Bit   ]])
if t == nil then gg.sleep(1)
else
if t[1] then hacka() end
if t[2] then hitbox() end
if t[3] then flash() end
if t[4] then smoke() end
if t[5] then spread() end
if t[6] then hackbb() end
if t[7] then hackc() end
if t[8] then aim() end
if t[9] then hack2() end
if t[10] then Exit() end
end
XGCK=-1
end




function hacka()
gg.setRanges(gg.REGION_CODE_APP)
gg.searchNumber("h01 00 50 E3 16 00 00 1A 00 00 55 E3 01 00 00 1A", gg.TYPE_BYTE, false, gg.SIGN_EQUAL, 0, -1)
gg.getResults(100000)
gg.editAll("h03 00 50 E3 16 00 00 1A 00 00 55 E3 01 00 00 1A", gg.TYPE_BYTE)
gg.clearResults()


gg.setRanges(gg.REGION_C_HEAP | gg.REGION_C_ALLOC | gg.REGION_C_DATA | gg.REGION_C_BSS | gg.REGION_PPSSPP | gg.REGION_ANONYMOUS)
gg.searchNumber("0", gg.TYPE_DWORD, false, gg.SIGN_EQUAL, 0, -1, 0)
gg.processResume()

local t = gg.getResults(100000, nil, nil, nil, nil, nil, nil, nil, nil)
gg.addListItems(t)
t = nil
gg.clearList()
gg.clearResults()
gg.toast('Radar ON')
end

function hitbox()
