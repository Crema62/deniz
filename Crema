k = gg.getResults(4)
gg.addListItems(k)
gg.clearResults()


on = '[OFF] '
off = '[ON] '
smoke = off



function START()

local menu = gg.multiChoice({
radar.. 'Radar',
hitbox.. 'Hitbox (Head)',
bitbox.. 'Hitbox (Body)',
stars.. 'Hide Stars',
fire.. 'Fire Rate (Only Pistol)',
spread.. 'No Spread',
smoke.. 'No Smoke',
aim.. 'No Aimpunch',
'❌EXIT❌'},
nil,
'💏TEAM CREMA💏')

if menu == nil then noselect()
else
if menu[9] then exit()
else
if menu[1] then
if radar == on then
radar = off
gg.getListItems(e)
e[1].value = '0360A0E3010050E3r'
gg.setValues(e)
gg.toast('Radar OFF')
else
radar = on
gg.getListItems(e)
e[1].value =  '0360A0E3030050E3r'
gg.setValues(e)
gg.toast('Radar ON')
end
end


if menu[2] then
if hitbox == on then
hitbox = off
gg.getListItems(a)
a[1].value =  '00000000r'
gg.setValues(a)
gg.getListItems(b)
b[1].value =  '010050E33800001Ar'
gg.setValues(b)
gg.toast('Hitbox Head OFF')
else
hitbox = on
gg.getListItems(a)
a[1].value =  '00009841r'
gg.setValues(a)
gg.getListItems(b)
b[1].value =  '000050E33800001Ar'
gg.setValues(b)
gg.toast('Hitbox Head ON')
end
end


if menu[3] then
if bitbox == on then
bitbox = off
gg.getListItems(c)
c[1].value =  '00000000r'
gg.setValues(c)
gg.getListItems(d)
d[1].value =  '010050E33800001Ar'
gg.setValues(d)
gg.toast('Hitbox Body OFF')
else
bitbox = on
gg.getListItems(c)
c[1].value =  '1,068,200,512'
gg.setValues(c)
gg.getListItems(d)
d[1].value =  '000050E33800001Ar'
gg.setValues(d)
gg.toast('Hitbox Body ON')
end
end

if menu[6] then
if spread == on then
spread = off
gg.getListItems(f)
f[1].value = '0'
gg.setValues(f)
gg.toast('No Spread OFF')
else
spread = on
gg.getListItems(f)
f[1].value =  '3.14159274101F'
gg.setValues(f)
gg.toast('No Spread ON')
end
end


if menu[4] then
if stars == on then
stars = off
gg.getListItems(g)
g[1].value = '44'
gg.setValues(g)
gg.toast(' Hiding  Stars OFF')
else
stars = on
gg.getListItems(g)
g[1].value =  '999999'
gg.setValues(g)
gg.toast('Hiding  Stars ON')
end
end


if menu[8] then
if aim == on then
aim = off
gg.getListItems(h)
h[1].value = 'F0482DE910B08DE2r'
gg.setValues(h)
gg.toast('No Aimpunch OFF')
else
aim = on
gg.getListItems(h)
h[1].value = '00F020E31EFF2FE1r'
gg.setValues(h)
gg.toast('No AimPunch ON')
end
end



if menu[5] then
if fire == on then
fire = off
gg.getListItems(i)
i[1].value =  '00007042r'
gg.setValues(i)
gg.getListItems(j)
j[1].value =  '00007042r'
gg.setValues(j)
gg.toast('Fire Rate OFF')
else
fire = on
gg.getListItems(i)
i[1].value =  '00005C42r'
gg.setValues(i)
gg.getListItems(j)
j[1].value =  '00000C42r'
gg.setValues(j)
gg.toast('Fire Rate ON')
end
end



if menu[7] then
if smoke == on then
smoke = off
gg.getListItems(k)
k[1].value = '0AD7233Cr'
k[2].value = '0AD7233Cr'
k[3].value = '0AD7233Cr'
k[4].value = '0AD7233Cr'
gg.setValues(k)
gg.setValues(k)
gg.setValues(k)
gg.setValues(k)
gg.toast('No Smoke OFF')
else
smoke = on
gg.getListItems(k)
k[1].value = '000010C1r'
k[2].value = '000010C1r'
k[3].value = '000010C1r'
k[4].value = '000010C1r'
gg.setValues(k)
gg.setValues(k)
gg.setValues(k)
gg.setValues(k)
gg.toast('No Smoke ON')
end
end



end
end
menuk =-1
end

function noselect()
gg.toast('🌟 BY CREMA 🌟')
end

